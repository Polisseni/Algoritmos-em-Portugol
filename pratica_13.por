algoritmo "Tabuada"

var
   numero, i, resultado: inteiro

inicio
   escreva("Digite um número: ")
   leia(numero)

   para i de 1 ate 10 faca
      resultado <- numero * i
      escreval(numero, " x ", i, " = ", resultado)
   fimpara

fimalgoritmo

