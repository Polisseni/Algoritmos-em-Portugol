algoritmo "SomadorNumerico"

var

contador, n, s, maior: inteiro

inicio

s <- 0
contador <- 1
enquanto (contador <= 10) faca
escreval ("Digite o", cont, "o. valor: ")
leia (n)
se (n > maior) entao
    maior <- n
fimse
s <- s + n
contador <- contador + 1
fimenquanto
escreval ("A soma de todos os elementos digitados e igual a ", s)
escreval ("O maior valor digitado foi ", maior)

fimalgoritmo